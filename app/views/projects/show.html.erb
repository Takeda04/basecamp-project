<%= render 'form' %>
<div class="peo">
<div class="project-name"><h1><%= @project.name %></h1></div>
    <div class="edit-delete">
      <button type="button"  data-bs-toggle="modal" data-bs-target="#exampleModal"  class="project-edit">Edit Project</button>
      <%= button_to "Destroy this project", @project, method: :delete , class:"project-delete bg-danger"  %> 

      <p><%= link_to "Attachment", new_project_upl_path(@project), class: "project-attach bg-info"  %></p> <br />
      <p><%= link_to "Posts", project_posts_path(@project), class: "project-attach bg-light" %></p> <br />
      <p><%= link_to "Tasks", new_project_task_path(@project), class: "project-tasks bg-warning" %></p>
      <%= link_to "Back", root_path , class: "back1"%>
      
      


    </div>
   
</div>